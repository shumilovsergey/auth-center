# Public URL of auth-center (what the user's browser is sent to)
AUTH_URL=http://localhost:8886

# Internal URL for server-to-server /exchange call
# Same as AUTH_URL when running locally without Docker networking
AUTH_INTERNAL=http://localhost:8886

# Public URL of this client app (auth-center redirects back here)
APP_URL=http://localhost:8890

# Flask session secret
SECRET_KEY=dev-secret
